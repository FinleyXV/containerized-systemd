{
  "name": "Arch Linux with systemd",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "runArgs": [
    "--privileged",
    "-v", "/sys/fs/cgroup:/sys/fs/cgroup:ro",
    "--tmpfs", "/run",
    "--tmpfs", "/run/lock"
  ],
  "overrideCommand": false,
  "containerUser": "root",
  "postStartCommand": "systemctl daemon-reexec || true",
  "remoteUser": "root"
}
